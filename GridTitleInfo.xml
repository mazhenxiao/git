    </GridTitle>
  </PriceTitleFrozenColumns>
  <!--价格列表滚动头部-->
  <!--价格列表滚动头部-->
  <PriceTitleColumns>
    <GridTitle>
      <!--表头行数-->
      <TitleRowCount>1</TitleRowCount>
      <!--表头行属性，rindex为行号-->
      <TitleRowInfo rindex ="1" field ="ISHAVEPROPERTY" align="center" title="是否有产权" colspan="1" rowspan ="1" width="120" fixeded ="true" />
      <TitleRowInfo rindex ="1" field ="ISDECORATION" align="center" title="是否精装" colspan="1" rowspan ="1" width="120" fixeded ="true" />
